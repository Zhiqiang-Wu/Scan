{
    "$schema": "../node_modules/@tauri-apps/cli/schema.json",
    "build": {
        "beforeBuildCommand": "",
        "beforeDevCommand": "",
        "devPath": "http://localhost:8000",
        "distDir": "../dist"
    },
    "package": {
        "productName": "Scan",
        "version": "../package.json"
    },
    "tauri": {
        "allowlist": {
            "all": false
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
